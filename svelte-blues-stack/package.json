		"svelte": "^3.50.1",
		"svelte-check": "^3.0.0",
		"svelte-loader": "^3.1.3",
		"svelte-preprocess": "^4.10.6",
		"tslib": "^2.3.1",
		"typescript": "^4.7.4",
		"vite": "^4.0.1",
		"vitest": "^0.26.0"
	},
	"type": "module",
	"packageManager": "pnpm@7.22.0"
