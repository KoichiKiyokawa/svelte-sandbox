{
	"name": "svelte-kit-prisma",
	"version": "0.0.1",
	"scripts": {
		"postinstall": "prisma generate",
		"dev": "svelte-kit dev",
		"build": "svelte-kit build",
		"package": "svelte-kit package",
		"preview": "svelte-kit preview",
		"check": "svelte-check --tsconfig ./tsconfig.json --fail-on-warnings --fail-on-hints",
		"check:watch": "pnpm run check -- --watch",
		"lint": "prettier --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --write --plugin-search-dir=. .",
		"seed": "esno prisma/seed.ts"
	},
	"dependencies": {
		"@prisma/client": "3.10.0",
		"dayjs": "1.10.8",
		"felte": "1.0.1",
		"svelte-kit-cookie-session": "^2.1.2",
		"zod": "3.13.4"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "1.0.0-next.31",
		"@sveltejs/kit": "1.0.0-next.294",
		"@types/bcryptjs": "^2.4.2",
		"@types/node": "16.11.26",
		"@typescript-eslint/eslint-plugin": "5.13.0",
		"@typescript-eslint/parser": "5.13.0",
		"bcryptjs": "^2.4.3",
		"carbon-icons-svelte": "10.44.4",
		"eslint": "8.10.0",
		"eslint-config-prettier": "8.5.0",
		"eslint-plugin-svelte3": "3.4.1",
		"esno": "0.14.1",
		"prettier": "2.5.1",
		"prettier-plugin-svelte": "2.6.0",
		"prisma": "3.10.0",
		"svelte": "3.46.4",
		"svelte-check": "2.4.5",
		"svelte-preprocess": "4.10.4",
		"tslib": "2.3.1",
		"typescript": "4.6.2",
		"vite-plugin-windicss": "1.8.3",
		"windicss": "3.5.1"
	},
	"type": "module",
	"packageManager": "pnpm@6.32.3"
}
