{
	"name": "svelte-kit-prisma",
	"version": "0.0.1",
	"scripts": {
		"postinstall": "prisma generate",
		"dev": "svelte-kit dev",
		"build": "svelte-kit build",
		"package": "svelte-kit package",
		"preview": "svelte-kit preview",
		"check": "svelte-check --tsconfig ./tsconfig.json --fail-on-warnings --fail-on-hints",
		"check:watch": "pnpm run check -- --watch",
		"lint": "prettier --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --write --plugin-search-dir=. .",
		"seed": "esno prisma/seed.ts"
	},
	"dependencies": {
		"@prisma/client": "3.15.2",
		"dayjs": "1.11.3",
		"felte": "1.2.2",
		"svelte-kit-cookie-session": "3.0.5",
		"zod": "3.17.3"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "1.0.0-next.53",
		"@sveltejs/kit": "1.0.0-next.357",
		"@types/bcryptjs": "2.4.2",
		"@types/node": "18.0.0",
		"@typescript-eslint/eslint-plugin": "5.30.0",
		"@typescript-eslint/parser": "5.30.0",
		"bcryptjs": "2.4.3",
		"carbon-icons-svelte": "11.1.0",
		"eslint": "8.18.0",
		"eslint-config-prettier": "8.5.0",
		"eslint-plugin-svelte3": "4.0.0",
		"esno": "0.16.3",
		"prettier": "2.7.1",
		"prettier-plugin-svelte": "2.7.0",
		"prisma": "3.15.2",
		"svelte": "3.48.0",
		"svelte-check": "2.8.0",
		"svelte-preprocess": "4.10.7",
		"tslib": "2.4.0",
		"typescript": "4.7.4",
		"vite": "2.9.13",
		"vite-plugin-windicss": "1.8.6",
		"windicss": "3.5.6"
	},
	"type": "module",
	"packageManager": "pnpm@7.4.1"
}
