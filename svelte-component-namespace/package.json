		"eslint": "8.57.1",
		"eslint-config-prettier": "8.10.0",
		"eslint-plugin-svelte3": "4.0.0",
		"prettier": "2.8.8",
		"prettier-plugin-svelte": "2.10.1",
		"svelte": "3.59.2",
		"svelte-check": "3.8.6",
		"svelte-preprocess": "4.10.7",
		"tslib": "2.8.0",
